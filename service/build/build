#!/bin/sh

echo "build & install deepin-download-service"

cd ../
go build -o deepin-download-service

rm -rf ../dbus-factory/go
rm -rf ../dbus-factory/qml
cd ./dbus-factory/in.json/
../../build/json_build.py

cd ../../build
sudo ./install

rm -rf ../dbus-factory/go
rm -rf ../dbus-factory/qml
rm -rf ../deepin-download-service
