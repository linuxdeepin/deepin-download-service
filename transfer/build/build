#!/bin/sh

tranferBuild=$PWD

echo "build & install transfer"

cd ../
go build -o transfer

rm -rf ../dbus-factory/go
rm -rf ../dbus-factory/qml
cd ./dbus-factory/in.json/
../../build/json_build.py

cd $tranferBuild
sudo ./install

rm -rf ../dbus-factory/go
rm -rf ../dbus-factory/qml
rm -rf ../transfer
